{
  "name": "webpack-starter-config",
  "version": "2.0.0",
  "description": "Webpack boilerplate",
  "main": "index.js",
  "keywords": [
    "webpack",
    "es6",
    "babel",
    "eslint",
    "sass"
  ],
  "author": "Andrei Shved",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svensktutby/webpack-starter-config.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development node \"webpack/start.js\"",
    "build": "cross-env-shell NODE_ENV=production node \"webpack/build.js\"",
    "analyze": "webpack-bundle-analyzer \"./build/stats.json\"",
    "serve": "serve \"./build\" --single",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webpack": "^5.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "svg-sprite-loader": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "normalize.css": "^8.0.1"
  }
}
